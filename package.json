{
  "name": "clausejs-src",
  "version": "0.1.1",
  "description": "A powerful & flexible validation and conformation library for your JavaScript data and functions.",
  "main": "packages/clausejs/dist/clausejs.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "dev": "npm run test:watch",
    "dev-docsite": "webpack-dev-server --content-base ./docs --inline --host 0.0.0.0",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "cross-env CLAUSEJS_ENV=production webpack && cross-env CLAUSEJS_ENV=production MINIFY_ME=1 webpack && npm run build-docsite",
    "build-docsite": "babel-node scripts/build-docs.js",
    "lint": "eslint ./src"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": "<rootDir>(test/).*\\.(test|spec)\\.(ts)$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clausejs/clausejs.git"
  },
  "keywords": [
    "babel",
    "testing",
    "clause",
    "javascript",
    "clausejs",
    "validation",
    "schema"
  ],
  "author": {
    "name": "Xiyang Chen",
    "email": "settinghead@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clausejs/clausejs/issues"
  },
  "homepage": "https://github.com/clausejs/clausejs#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/jest": "^18.1.1",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "awesome-typescript-loader": "^3.0.7",
    "babel-cli": "^6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-register": "6.18.0",
    "bootstrap": "4.0.0-alpha.6",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "codemirror": "^5.23.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.0",
    "eslint-config-idiomatic": "^2.1.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "highlight.js": "^9.9.0",
    "jest": "^19.0.2",
    "lodash": "4.17.4",
    "markdown-include": "^0.4.3",
    "mocha": "^3.0.2",
    "mocha-testcheck": "0.1.2",
    "raw-loader": "^0.5.1",
    "showdown": "^1.5.5",
    "style-loader": "^0.13.1",
    "testcheck": "^0.1.4",
    "ts-node": "^2.1.0",
    "typescript": "^2.2.1",
    "typings": "^2.1.0",
    "webpack": "2.2.0-rc.2",
    "webpack-dev-server": "2.2.0-rc.0"
  }
}
